<resources>
	<style name="web_icon">
		<item name="android:layout_width">fill_parent</item>
		<item name="android:layout_height">64dp</item>
		<item name="android:scaleType">fitCenter</item>
		<item name="android:layout_weight">1</item>
		<item name="android:background">@drawable/re_web_icon_background</item>
	</style>
	<style name="item">
		<item name="android:layout_width">fill_parent</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:layout_weight">1</item>
		<item name="android:background">@drawable/re_item_bg</item>
		<item name="android:layout_margin">5dp</item>
		<item name="android:padding">5dp</item>
		<item name="android:gravity">center</item>
	</style>
	<style name="item.nopadding">
		<item name="android:padding">0dp</item>
	</style>
		<style name="plan">
   		<item name="android:orientation">horizontal</item>
	    <item name="android:layout_width">fill_parent</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:layout_weight">1</item>
		<item name="android:layout_margin">5dp</item>
   		<item name="android:textSize">18sp</item>
	</style>
	<style name="price">
	    <item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
   		<item name="android:textSize">18sp</item>
   		<item name="android:textColor">@color/main_color</item>
   		<item name="android:layout_marginRight">10dp</item>
   		<item name="android:layout_alignParentRight">true</item>
	</style>
    <style name="divider">
   		<item name="android:orientation">vertical</item>
        <item name="android:layout_width">fill_parent</item>
   		<item name="android:layout_height">1dp</item>
   		<item name="android:background">@color/divider</item>
	</style>
    <style name="account_txt">
   		<item name="android:layout_width">wrap_content</item>
   		<item name="android:layout_height">wrap_content</item>
   		<item name="android:textSize">18sp</item>
   		<item name="android:layout_marginBottom">5dp</item>
   	</style>
    <style name="box">
   		<item name="android:layout_width">match_parent</item>
   		<item name="android:layout_height">match_parent</item>
   		<item name="android:background">@drawable/re_box_bg</item>
   		<item name="android:layout_marginTop">9dp</item>
   		<item name="android:layout_marginBottom">4dp</item>
   		<item name="android:layout_marginLeft">3dp</item>
   		<item name="android:layout_marginRight">3dp</item>
   	</style>
    <style name="nav_btn">
   		<item name="android:layout_width">match_parent</item>
   		<item name="android:layout_height">wrap_content</item>
   		<item name="android:layout_weight">1</item>
   		<item name="android:background">@color/none</item>
		<item name="android:textColor">#99333333</item>
		<item name="android:textSize">18sp</item>
	</style>
    <style name="ProgressBar" parent="@android:style/Widget.Holo.Light.ProgressBar.Horizontal">
        <item name="android:indeterminateOnly">false</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">6dp</item>
        <item name="android:indeterminate">true</item>
    </style>
    <style name="flag">
   		<!-- <item name="android:adjustViewBounds">true</item>
   		<item name="android:background">@color/plan_back</item>
   		<item name="android:padding">1dp</item> -->
	</style>
    <style name="ratingBar" parent="@android:style/Widget.Holo.Light.RatingBar.Small">
		<item name="android:progressDrawable">@drawable/re_ratingbar_small</item>
        <item name="android:minHeight">16dip</item>
        <item name="android:maxHeight">16dip</item>        
    </style>
</resources>